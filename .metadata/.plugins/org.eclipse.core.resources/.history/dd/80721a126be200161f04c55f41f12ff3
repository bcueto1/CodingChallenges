
import static org.junit.Assert.*;

import org.junit.Test;

public class TripleLiftTest {
	
	/** Test multiple of 3. */
	@Test
	public void testMultipleThree() {
		String temp = "1\n"
				+ "2\n"
				+ "Triple\n"
				+ "4\n";
		assertEquals(temp, TripleLift.printNumbers(4));
	}
	
	/** Test multiple of 7. */
	public void testMultipleSeven() {
		String temp = "1\n"
				+ "2\n"
				+ "Triple\n"
				+ "4\n"
				+ "5\n"
				+ "Triple\n"
				+ "Lift\n";
	}

}
