
public class TripleLiftianNumbers {
	
	private static int t0 = 6;
	private static int t1 = 7;
	private static int t2 = 2;
	
	private static int solveEquation(int x, int y, int z) {
		return 4 * x - 5 * y + 3 * z;
	}
	
    public static int TripleLiftianNumber(int myNumber) {
        int solution = 0;
    	while (myNumber != 3) {
    	    t2 = TripleLiftianNumber(myNumber - 1);
    	}
    	solution = solveEquation(t0, t1, t2);
    	t0 = t1;
    	t1 = t2;
    	return solution;
    }
    
    public static void main(String[] args) {
        System.out.println(TripleLiftianNumber(3));
    }
}
