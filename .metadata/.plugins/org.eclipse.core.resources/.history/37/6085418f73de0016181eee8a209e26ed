
public class TripleLiftianNumbers {

	private static long t0 = 6;
	private static long t1 = 7;
	private static long t2 = 2;
	
    public static long getNumber(int myNumber) {
        long temp = 0;
    	if (myNumber != 3) {
    	    temp = getNumber(myNumber - 1);
    	    t0 = t1;
        	t1 = t2;
        	t2 = temp;
    	}
    	return (4 * t2) - (5 * t1) + (3 * t0);
    }
    
    public static String TripleLiftianNumber(int myNumber) {
        return Long.toString(getNumber(myNumber));
    }
    
    public static void main(String[] args) {
    	System.out.println(TripleLiftianNumber(25));
    }
}
